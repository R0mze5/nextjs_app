{
  "name": "nextjs_app",
  "author": "Roman Khoroshilov <roman.rs.kh@gmail.com>",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "keywords": [
    "next",
    "nextjs",
    "react",
    "typescript",
    "redux",
    "redux-saga",
    "amp",
    "eslint",
    "nodejs",
    "sw",
    "pwa",
    "styled-components"
  ],
  "engines": {
    "node": ">=14.5",
    "npm": ">=6.14",
    "yarn": ">=1.22"
  },
  "main": "server/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "next build",
    "start": "next start",
    "export": "npm run build && next export",
    "type-check": "tsc --noEmit",
    "start:ssr": "npm run build && cross-env NODE_ENV=production node ./server/index.js",
    "start:static": "npm run export && cross-env NODE_ENV=production node ./server/static",
    "serve": "serve -p 8080 ./out",
    "watch-ts": "tsc -w",
    "release": "standard-version",
    "prerelease": "standard-version -- --prerelease",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/react-redux": "^7.1.9",
    "@types/styled-components": "^5.1.2",
    "@welldone-software/why-did-you-render": "^5.0.0-alpha.2",
    "chalk": "^4.1.0",
    "connected-next-router": "^3.1.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.5.2",
    "next-offline": "^5.0.2",
    "next-redux-wrapper": "^6.0.2",
    "nextjs-progressbar": "^0.0.6",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/express": "^4.17.7",
    "@types/next": "^9.0.0",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.49",
    "@types/redux-actions": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "react-is": "^16.13.1",
    "serve": "^11.3.2",
    "standard-version": "^9.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
